{{#each rows}}
<div class="card text-center position-absolute top-50 start-50 translate-middle" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{first_name}} {{last_name}}</h5>
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">Telefono: {{phone}}</li>
    <li class="list-group-item">Correo: {{email}}</li>
  </ul>
  <div class="card-body">
    <a href="/adopt" class="btn btn-danger">Volver</a>
  </div>
</div>
{{/each}}